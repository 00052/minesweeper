#include "resource.h"
#include <windows.h>

STRINGTABLE
BEGIN
	IDS_1 "Hello World"
END

IDI_APP ICON "github.ico"

1 24 "manifest.xml"
  
IDD_CONFIG_DIALOG DIALOGEX 0, 0, 220, 80        //DIALOGEX RESOURCE  
STYLE DS_SETFONT | DS_MODALFRAME | DS_FIXEDSYS | WS_POPUP | WS_CAPTION | WS_SYSMENU  | WS_VISIBLE
